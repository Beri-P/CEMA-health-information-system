# Server port
PORT=3000

# PostgreSQL connection
DB_DATABASE=cema_db
DB_USERNAME=postgres
DB_PASSWORD=your_db_password
DB_HOST=localhost
DB_PORT=5432
DB_DIALECT=postgres

# Alternative: Use DATABASE_URL instead of individual DB settings
# DATABASE_URL=postgres://user:password@localhost:5432/cema_db

# JWT settings
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h